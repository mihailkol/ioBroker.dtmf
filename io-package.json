{
  "common": {
    "name": "dtmf",
    "version": "0.0.1",
    "title": "DTMF Adapter",
    "desc": "Adapter for DTMF control",
    "platform": "Javascript/Node.js",
    "mode": "daemon",
    "icon": "dtmf.png",
    "enabled": true,
    "keywords": [ "dtmf", "modem" ],
    "type": "communication"
  },
  "native": {
    "modemPort": {
      "type": "string",
      "default": "/dev/ttyUSB0",
      "required": true
    },
    "modemBaudRate": {
      "type": "number",
      "default": 9600,
      "required": true
    },
    "users": {
      "type": "array",
      "default": []
    },
    "devices": {
      "type": "array",
      "default": []
    }
  }
}